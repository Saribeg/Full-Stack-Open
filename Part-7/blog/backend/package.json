{
  "name": "blog",
  "version": "1.0.0",
  "description": "Full Stack Open course, part 4, blog list",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development node --watch index.js",
    "start:test": "cross-env NODE_ENV=test node index.js",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test node --test --test-concurrency=1",
    "test:single": "npm test -- --test-only --test-concurrency=1",
    "test:coverage": "c8 --config .c8rc.json npm test",
    "test:coverage:check": "c8 --config .c8rc.json check-coverage --per-file",
    "test:coverage:report": "c8 --config .c8rc.json report",
    "check:dead": "knip --exports --dependencies --no-exit-code",
    "audit": "npm audit --audit-level=critical"
  },
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "envalid": "^8.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@stylistic/eslint-plugin": "^5.0.0",
    "c8": "^10.1.3",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "knip": "^5.63.0",
    "supertest": "^7.1.1"
  }
}
