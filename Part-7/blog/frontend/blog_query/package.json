{
  "name": "bloglist-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start:backend:dev": "cd ../../backend && npm run dev",
    "start:backend:test": "cd ../../backend && npm run start:test",
    "start:full:dev": "concurrently -k -n BACKEND,FRONTEND -c yellow,cyan \"npm run start:backend:dev\" \"npm run dev\"",
    "start:full:test": "concurrently -k -n BACKEND,FRONTEND -c yellow,cyan \"npm run start:backend:test\" \"npm run dev\"",
    "build": "vite build",
    "check:bundle": "npx bundlesize2",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:components": "vitest run tests/components",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "cypress run --e2e --browser chrome --reporter cypress-mochawesome-reporter --reporter-options reportDir=cypress/reports,overwrite=false,html=false,json=true",
    "test:e2e:ui": "cypress open --e2e",
    "test:e2e:headed": "cypress run --e2e --browser chrome --headed --reporter cypress-mochawesome-reporter --reporter-options reportDir=cypress/reports,overwrite=false,html=false,json=true",
    "test:e2e:report": "mochawesome-merge cypress/reports/*.json > cypress/reports/merged.json && mochawesome-report-generator cypress/reports/merged.json -o cypress/reports/html",
    "test:e2e:dev": "cross-env CYPRESS=true start-server-and-test start:full:test http://127.0.0.1:5173 \"cypress run --e2e --browser chrome\"",
    "test:e2e:ui:dev": "cross-env CYPRESS=true start-server-and-test start:full:test http://127.0.0.1:5173 \"cypress open --e2e\"",
    "test:e2e:headed:dev": "cross-env CYPRESS=true start-server-and-test start:full:test http://127.0.0.1:5173 \"cypress run --e2e --browser chrome --headed\"",
    "test:e2e:report:dev": "mochawesome-merge cypress/reports/*.json > cypress/reports/merged.json && mochawesome-report-generator cypress/reports/merged.json -o cypress/reports/html && http-server cypress/reports/html -o -p 7333",
    "test:e2e:ci": "npm run test:e2e || EXIT=$?; npm run test:e2e:report || true; exit ${EXIT:-0}",
    "check:dead": "knip --exports --dependencies --no-exit-code",
    "audit": "npm audit --audit-level=critical",
    "check:outdated": "npm outdated"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.6",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "axios": "^1.4.0",
    "knip": "^5.63.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "bundlesize2": "^0.0.35",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "cypress": "^14.5.4",
    "cypress-mochawesome-reporter": "^4.0.2",
    "eslint": "^9.30.0",
    "eslint-plugin-cypress": "^5.1.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.3.0",
    "http-server": "^14.1.1",
    "jsdom": "^26.1.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "msw": "^2.10.5",
    "rollup-plugin-visualizer": "^6.0.3",
    "start-server-and-test": "^2.0.13",
    "vite": "^4.4.5",
    "vitest": "^3.2.4"
  }
}
